<h1> Add your portfolio</h1>

<%=form_for @portfolio do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %> <br><br>
  <%= f.label :subtitle %>
  <%= f.text_field :subtitle %><br><br>
  <%= f.label :image %>
  <%= f.text_field :image %><br><br>
  <%=f.submit%>
  <%=link_to "Back", portfolios_path %>

<%end %>
